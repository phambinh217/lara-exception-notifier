{
    "name": "phambinh/lara-exception-notifier",
    "description": "Notify when your laravel project has exception",
    "keywords": ["framework", "laravel", "notify", "exception"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Phambinh\\LaraExceptionNotifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phambinh\\LaraExceptionNotifier\\ServiceProvider"
            ]
        }
    }
}